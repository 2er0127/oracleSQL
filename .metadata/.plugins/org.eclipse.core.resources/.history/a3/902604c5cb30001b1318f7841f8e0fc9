			String sql=("select *from emp order by empno ASC");
			
			pstmt=conn.prepareStatment(sql);
			rs.pstmt.executeQuery(); //while을 가지고 db에서 데이터를 수행하기 위한 객체 변수
			
			double bonus=0.0; // 보너스를 구하기 위한 변
			int pay=0; //실지급액
			
			
		}

